let arr = input.split("");
console.log(arr);

let count = arr.reduce((a, e) => {
  return e == "(" ? a + 1 : a - 1;
}, 0);

count = 0;
index = 0;
while (count >= 0) {
  if (input[index] == "(") {
    count = count + 1;
  } else {
    count = count - 1;
  }
  index++;
}
console.log(index);
